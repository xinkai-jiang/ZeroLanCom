{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Debug C++ (GDB)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/examples/custom_message_example",
      // "args": ["./local_test.yaml"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "MIMode": "gdb",
      "setupCommands": [
        { "description": "Enable pretty printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
      ],
      "externalConsole": false
    }
  ]
}
